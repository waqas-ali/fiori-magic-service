<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
		<application>convergentapi</application>
        <version>1</version>
        <threadsafe>true</threadsafe>
        <system-properties>
       	<property name="odata4j.producerfactory" value="org.odata4j.appengine.DatastoreProducerFactory" />
       	<property name="odata4j.datastore.namespace" value="convergent" />
       	<property name="appengine-port" value="8080" />
	    </system-properties>
    	<sessions-enabled>true</sessions-enabled>
</appengine-web-app>